{
  "name": "technical-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint:scripts": "eslint src/**/*.{js,jsx,ts,tsx}",
    "fix:scripts": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "lint:styles": "stylelint src/**/*.{css,scss}",
    "fix:styles": "stylelint --fix src/**/*.{css,scss}",
    "lint:prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss}",
    "fix:prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss}",
    "fix": "npm run fix:scripts & npm run fix:styles & npm run fix:prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-prettier": "^4.0.2",
    "tailwindcss": "^3.3.3"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": "please use yarn",
    "yarn": ">= 1.19.1"
  }
}
